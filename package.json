{
  "name": "markdown-enhanced",
  "displayName": "Markdown Enhanced",
  "description": "Markdown Enhanced for vscode",
  "version": "0.3.0",
  "license": "SEE LICENSE IN LICENSE",
  "author": "Tarocch1 <huiben.fang@gmail.com> (https://github.com/Tarocch1)",
  "homepage": "https://github.com/Tarocch1/vscode-markdown-enhanced#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/Tarocch1/vscode-markdown-enhanced.git"
  },
  "bugs": "https://github.com/Tarocch1/vscode-markdown-enhanced/issues",
  "publisher": "Tarocch1",
  "icon": "images/logo.png",
  "engines": {
    "vscode": "^1.62.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:markdown"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "markdown.markdownItPlugins": true,
    "markdown.previewStyles": [
      "./styles/base.css",
      "./styles/github-markdown.css",
      "./styles/github-highlight.css"
    ],
    "configuration": {
      "title": "Markdown Enhanced",
      "properties": {
        "markdownEnhanced.theme": {
          "type": "string",
          "default": "light",
          "enum": [
            "light",
            "dark"
          ],
          "enumDescriptions": [
            "%markdownEnhanced.configuration.theme.light%",
            "%markdownEnhanced.configuration.theme.dark%"
          ],
          "description": "%markdownEnhanced.configuration.theme%"
        },
        "markdownEnhanced.abbr.enable": {
          "type": "boolean",
          "default": true,
          "description": "%markdownEnhanced.configuration.abbr.enable%"
        },
        "markdownEnhanced.anchor.enable": {
          "type": "boolean",
          "default": true,
          "description": "%markdownEnhanced.configuration.anchor.enable%"
        },
        "markdownEnhanced.anchor.permalink": {
          "type": "boolean",
          "default": true,
          "description": "%markdownEnhanced.configuration.anchor.permalink%"
        },
        "markdownEnhanced.attrs.enable": {
          "type": "boolean",
          "default": true,
          "description": "%markdownEnhanced.configuration.attrs.enable%"
        },
        "markdownEnhanced.deflist.enable": {
          "type": "boolean",
          "default": true,
          "description": "%markdownEnhanced.configuration.deflist.enable%"
        },
        "markdownEnhanced.footnote.enable": {
          "type": "boolean",
          "default": true,
          "description": "%markdownEnhanced.configuration.footnote.enable%"
        },
        "markdownEnhanced.mark.enable": {
          "type": "boolean",
          "default": true,
          "description": "%markdownEnhanced.configuration.mark.enable%"
        },
        "markdownEnhanced.sub.enable": {
          "type": "boolean",
          "default": true,
          "description": "%markdownEnhanced.configuration.sub.enable%"
        },
        "markdownEnhanced.sup.enable": {
          "type": "boolean",
          "default": true,
          "description": "%markdownEnhanced.configuration.sup.enable%"
        },
        "markdownEnhanced.tableOfContents.enable": {
          "type": "boolean",
          "default": true,
          "description": "%markdownEnhanced.configuration.tableOfContents.enable%"
        },
        "markdownEnhanced.tableOfContents.includeLevel": {
          "type": "array",
          "default": [
            2,
            3,
            4
          ],
          "maxItems": 6,
          "minItems": 0,
          "items": {
            "type": "number"
          },
          "uniqueItems": true,
          "description": "%markdownEnhanced.configuration.tableOfContents.includeLevel%"
        },
        "markdownEnhanced.taskLists.enable": {
          "type": "boolean",
          "default": true,
          "description": "%markdownEnhanced.configuration.taskLists.enable%"
        },
        "markdownEnhanced.taskLists.readonly": {
          "type": "boolean",
          "default": true,
          "description": "%markdownEnhanced.configuration.taskLists.readonly%"
        },
        "markdownEnhanced.taskLists.label": {
          "type": "boolean",
          "default": true,
          "description": "%markdownEnhanced.configuration.taskLists.label%"
        },
        "markdownEnhanced.underline.enable": {
          "type": "boolean",
          "default": true,
          "description": "%markdownEnhanced.configuration.underline.enable%"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/markdown-it": "^12.2.3",
    "@types/node": "^16.11.11",
    "@types/vscode": "^1.62.0",
    "markdown-it": "^12.2.0",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "github-markdown-css": "^5.0.0",
    "highlight.js": "^11.3.1",
    "markdown-it-abbr": "^1.0.4",
    "markdown-it-anchor": "^8.4.1",
    "markdown-it-attrs": "^4.1.0",
    "markdown-it-deflist": "^2.1.0",
    "markdown-it-footnote": "^3.0.3",
    "markdown-it-mark": "^3.0.1",
    "markdown-it-sub": "^1.0.0",
    "markdown-it-sup": "^1.0.0",
    "markdown-it-table-of-contents": "^0.6.0",
    "markdown-it-task-lists": "^2.1.1",
    "markdown-it-underline": "^1.0.1"
  }
}
